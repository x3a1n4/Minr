# renderSelection()
# For all blocks, render a single particle

@for String sBlockLoc in this.blocks.keys
    @define String[] b = sBlockLoc.split(" ")
    @define BlockLocation block = BlockLocation(b[0], b[1], b[2], b[3])
    @bypass /execute in minecraft:{{block.getWorld().toLowerCase()}} run particle minecraft:angry_villager {{block.getX()}} {{block.getY()()}} {{block.getZ()}} 0 0 0 1 1
@done