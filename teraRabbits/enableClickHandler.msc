@player &6&lStarting teraRabbits!
# Summon a massive ghast at player location, and attach the script to it
@define String targetName = "{{player.getName()}} teraRabbits target"
@bypass /summon ghast ~ ~ ~ {ExplosionPower:0,CustomName:"{{targetName}}",NoAI:1b,PersistenceRequired:1b,Silent:1b,active_effects:[{id:invisibility,duration:999999,show_particles:0b}],attributes:[{id:scale,base:16f}]}
# Get UUID of ghast
@bypass /script import entity {{Entity(player.getX(), player.getY(), player.getZ(), player.getWorld()}} scriptname